{
  "name": "fishbowl-client",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run css",
    "css-compile": "sass styles.scss ./../wwwroot/css/styles.css --load-path=node_modules --style compressed",
    "css-prefix": "postcss --replace ./../wwwroot/css/styles.css --use autoprefixer --map",
    "css-purge": "purgecss -c ./purgecss.config.js",
    "css": "npm-run-all css-compile css-prefix css-purge"
  },
  "dependencies": {
    "bootstrap": "^5.0.0-beta3"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.10",
    "postcss-cli": "^8.3.1",
    "purgecss": "^4.0.3",
    "sass": "^1.32.8"
  }
}
