@using Microsoft.Extensions.Logging;

@inject ILogger<StateManager> Logger;

<CascadingValue Value="this">
    @ChildContent

    @if (this.ActiveState is not null)
    {
        <div class="container-fluid w-100 h-100 bg-dark">
            @this.ActiveState.Container
        </div>
    }
</CascadingValue>