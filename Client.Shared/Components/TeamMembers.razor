@inject IStringLocalizer<Resources> L

<MudCard Class="mud-theme-success">
    <MudCardContent>
        <MudText Typo="Typo.h6">@L["Components.TeamMembers.Title"]</MudText>
        <MudText Typo="Typo.subtitle1">@string.Join(", ", this.Players.Select(player => player.Name))</MudText>
    </MudCardContent>
</MudCard>